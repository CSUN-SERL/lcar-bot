<launch>
	<!-- vim: set ft=xml noet : -->
	<!-- example launch script for PX4 based FCU's -->

	<arg name="fcu_url1" default="udp://localhost:14550@" />
	<arg name="gcs_url1" default="" />
	<arg name="tgt_system1" default="1" />
	<arg name="tgt_component1" default="1" />
	<arg name="log_output1" default="screen" />

	<include file="$(find rqt_gcs)/launch/node_UAV.launch">
		<arg name="pluginlists_yaml" value="$(find mavros)/launch/px4_pluginlists.yaml" />
		<arg name="config_yaml" value="$(find mavros)/launch/px4_config.yaml" />

		<arg name="fcu_url" value="$(arg fcu_url1)" />
		<arg name="gcs_url" value="$(arg gcs_url1)" />
		<arg name="tgt_system" value="$(arg tgt_system1)" />
		<arg name="tgt_component" value="$(arg tgt_component1)" />
		<arg name="namespace" value="UAV1" />
		<arg name="log_output" value="$(arg log_output1)" />
	</include>

</launch>
